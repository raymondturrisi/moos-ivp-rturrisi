//--------------------------------------------------------
// iM300 Config Block
	
ProcessConfig = iM300
{
  AppTick    = 10
  CommsTick  = 10

	comms_type = client
  	port       = 29500
	ip_addr    = $(FSEAT_IP)

	stale_thresh = 15
	
	max_rudder  = 50
	max_thrust  = 100
	drive_mode  = normal
	ignore_msg  = $GPGLL, $GPGSV, $GPVTG, $GPZDA, $CPALT, $CPIMU, $CPNVR, $GPGST, $CPRCM
	ignore_checksum_errors = true

	max_appcast_events       = 8
//	max_appcast_run_warnings = 10
	
	// Set to 1 if you want to use IMU heading for NAV_HEADING
	use_imu_heading = true

	nav_prefix 			= NAV
	gps_prefix			= GPS
	compass_prefix	= COMPASS

}

